{
  "name": "vote.gov",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "config": {
    "votegov": {
      "hugo": {
        "en": "config.json"
      },
      "urls": {
        "staging": "https://vote-gov-staging.apps.cloud.gov",
        "production": "https://vote.gov/"
      }
    }
  },
  "scripts": {
    "gulp": "gulp",
    "hugo": "hugo",
    "dev": "gulp dev",
    "federalist": "export NODE_ENV=production && npm run build",
    "build": "gulp buildWebsite",
    "start": "gulp website",
    "test": "gulp test",
    "deploy": "gulp deploy",
    "cy:open": "cypress open",
    "cy:test": "cypress run --spec 'cypress/integration/tests/**/*'",
    "cy:axe": "cypress run --spec 'cypress/integration/accessibility/**/*'",
    "htmlproofer": "htmlproofer tmp/public --allow-hash-href --check-html --empty-alt-ignore --disable-external"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/vote-gov.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/18F/vote-gov/issues"
  },
  "homepage": "https://github.com/18F/vote-gov#readme",
  "license": "CC0-1.0",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "autoprefixer": "^10.4.2",
    "browserify": "^17.0.0",
    "cross-spawn": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-dart-scss": "^1.1.0",
    "gulp-eslint": "^6.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "sass": "^1.48.0",
    "uswds": "^2.13.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "devDependencies": {
    "axe-core": "^4.4.2",
    "cypress": "^9.7.0",
    "cypress-axe": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "gulp-scss-lint": "^1.0.0",
    "hugo-bin": "^0.80.0"
  }
